{
  "name": "query-intelligence-tool",
  "version": "1.0.0",
  "description": "Query Intelligence Chrome Extension - Extract real demand signals from Google Suggest",
  "main": "src/chrome/background.js",
  "scripts": {
    "dev": "webpack --mode development --watch",
    "build:chrome": "webpack --mode production --env target=chrome",
    "build:firefox": "webpack --mode production --env target=firefox",
    "build:all": "npm run build:chrome && npm run build:firefox",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "release:major": "npm version major",
    "release:minor": "npm version minor",
    "release:patch": "npm version patch"
  },
  "keywords": [
    "chrome-extension",
    "query-intelligence",
    "seo",
    "keyword-research",
    "google-suggest",
    "perplexity",
    "openai",
    "gemini",
    "claude"
  ],
  "author": "Query Intelligence Team",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.0",
    "zip-webpack-plugin": "^4.0.1"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}
